}
QComboBox QAbstractItemView {
background-color: #505050;
border: 1px solid #e0e0e0;
color: #e0e0e0;
}

QComboBox QAbstractItemView:item:hover {
background-color: #707070;
color: #e0e0e0;
}

QComboBox QAbstractItemView:item:focus {
background-color: #707070;
color: #e0e0e0;
}
